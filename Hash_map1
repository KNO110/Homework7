using System;
using System.Collections.Generic;

class Program
{
    static void Main()
    {
        /// Ключ стринг у нас а значения инт
        Dictionary<string, int> hash = new Dictionary<string, int>();

        hash["лет строгого режима"] = 5;
        hash["пульт от ядерки"] = 3;
        hash["аболтус"] = 8;

        int aboltusCount = hash["аболтус"];           ///Получаем наше значение через ключ
        Console.WriteLine("Количество аболтусов в классе: " + aboltusCount);

        ///// Проверяем наличие ключа
        bool containsPear = hash.ContainsKey("двоешник");
        Console.WriteLine("Содержит двоешников: " + containsPear);

        // Перебор всех элементов хэш-таблицы
        foreach (var key in hash)
        {
            Console.WriteLine($"Ключ: {key.Key}; Значение: {key.Value}");
        }
    }
}
